{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "rules": {
        "no-unused-vars": ["error", { "varsIgnorePattern": "^_" }]
    }
}
