{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/api/:path*"
        }
    ],
    "crons": [
        {
            "path": "/v2/zdte/expire",
            "schedule": "1 8 * * *"
        }
    ]
}
